{
  "verbose": false,
  "poll_interval": 5,
  "max_request_rate": 15,
  "rate_limit_margin": 0.7,
  "log_level": "warn",
  "log_compress": true,
  "log_sidecar": true,
  "log_categories": {
    "logging": "trace",
    "history": "debug"
  },
  "http_timeout": 50,
  "http_retries": 4,
  "download_limit": 21,
  "upload_limit": 22,
  "max_download": 23,
  "max_upload": 24,
  "http_proxy": "http://proxy",
  "https_proxy": "http://secureproxy",
  "use_graphql": false
}

